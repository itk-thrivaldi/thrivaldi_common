    <joint name="${prefix}joint_e3" type="prismatic">
      <origin xyz="-0.164527 0.159154 -0.053205" rpy="${DEG2RAD * 90} 0 0"/>
      <parent link="link_trolley"/>
      <child link="link_arm"/>
      <axis xyz="0 0 1.0"/>
      <limit lower="-0.8" upper="0.8" effort="1.0" velocity="0"/>
    </joint>

    <joint name="${prefix}link_base" type="fixed">
      <origin xyz="0 0 0" rpy="0 ${DEG2RAD * 90} 0"/>
      <parent link="root"/>
      <child link="link_base"/>
    </joint>

    <joint name="${prefix}joint_e1" type="prismatic">
      <origin xyz="-2.570369 -0.057403 0.034558" rpy="0 0 0"/>
      <parent link="link_base"/>
      <child link="link_leg"/>
      <axis xyz="0 0 1.0"/>
      <limit lower="-0.77" upper="0.77" effort="1.0" velocity="0"/>
    </joint>

    <joint name="${prefix}joint_e2" type="prismatic">
      <origin xyz="-1.277963 0.430506 -0.476427" rpy="-${DEG2RAD * 90} -${DEG2RAD * 90} 0"/>
